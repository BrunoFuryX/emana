max-width: 1199px) {\n  .minicart--fixed {\n    width: 100%;\n    height: 100%;\n    overflow: hidden;\n  }\n  .minicart {\n    position: fixed;\n    z-index: 200;\n    top: 0;\n    left: 0;\n    width: 100%;\n    height: 100%;\n    background: rgba(0,0,0,0.5);\n    visibility: hidden;\n    opacity: 0;\n    transition: all 0.2s ease-in-out;\n  }\n  .minicart.minicart--actived {\n    visibility: visible;\n    opacity: 1;\n  }\n  .minicart__container {\n    position: fixed;\n    top: 0;\n    right: 0;\n    width: 300px;\n    height: 100%;\n    background: #fff;\n    transform: translateX(100%);\n    transition: all 0.2s ease-in-out;\n  }\n  .minicart--actived .minicart__container {\n    transform: translateX(0);\n  }\n  .minicart__header {\n    display: flex;\n    justify-content: space-between;\n    align-items: center;\n    margin: 0 10px;\n    border-bottom: 1px solid #f3f3f3;\n  }\n  .minicart__title {\n    color: #b53b96;\n    flex: 1 1 auto;\n    font-size: 14px;\n    font-weight: 700;\n    margin: 0;\n    text-transform: uppercase;\n  }\n  .minicart__close {\n    width: 45px;\n    height: 45px;\n    display: flex;\n    justify-content: center;\n    align-items: center;\n    color: #b53b96;\n    font-size: 20px;\n  }\n  .minicart__empty {\n    font-weight: 500;\n    text-align: center;\n    padding: 20px;\n  }\n  .minicart__list {\n    margin: 0;\n    padding: 0;\n  }\n  .minicart__item {\n    position: relative;\n    max-height: 130px;\n    visibility: visible;\n    opacity: 1;\n    transition: all 0.4s ease-in-out;\n  }\n  .minicart__item + .minicart__item {\n    border-top: solid 1px #eee;\n  }\n  .minicart__link {\n    display: flex;\n    align-items: center;\n    list-style: none;\n    padding: 10px;\n    color: #666;\n  }\n  .minicart__link:hover {\n    text-decoration: none;\n  }\n  .minicart__info {\n    display: flex;\n    flex-wrap: wrap;\n    justify-content: space-between;\n    align-items: center;\n  }\n  .minicart__name {\n    font-size: 14px;\n    flex: 0 0 100%;\n  }\n  .minicart__image {\n    flex: 0 0 50px;\n    margin-right: 20px;\n  }\n  .minicart__qty {\n    font-size: 10px;\n  }\n  .minicart__price {\n    font-size: 14px;\n    color: #f00;\n  }\n  .minicart__total {\n    display: flex;\n    height: 45px;\n    align-items: center;\n    justify-content: space-between;\n    font-size: 14px;\n    font-weight: 700;\n    padding: 0 20px;\n  }\n  .minicart__main {\n    height: calc(100% - 140px);\n    overflow-y: auto;\n  }\n  .minicart__finish {\n    background: #3aad27;\n    color: #fff;\n    font-size: 16px;\n    font-weight: 700;\n    text-transform: uppercase;\n    height: 50px;\n    display: flex;\n    justify-content: center;\n    align-items: center;\n  }\n  .minicart__finish:hover {\n    background: #339021;\n  }\n  .minicart__delete {\n    background: #fff;\n    border: 1px solid #e74949;\n    border-radius: 3px;\n    font-size: 10px;\n    font-weight: 600;\n    height: 25px;\n    line-height: 25px;\n    margin: 0;\n    max-height: 0;\n    opacity: 0;\n    overflow: hidden;\n    padding: 0 10px;\n    transition: all 150ms ease-in-out;\n    visibility: hidden;\n  }\n  .minicart__delete svg {\n    fill: #e74949;\n  }\n  .minicart__delete:hover {\n    background: #e74949;\n    color: #fff;\n  }\n  .minicart__delete:hover svg {\n    fill: #fff;\n  }\n  .minicart__qty:before {\n    content: ' / ';\n  }\n}\n@media (min-width: 1200px) {\n  #cart .dropdown:hover .cart__dropdown {\n    opacity: 1;\n    visibility: visible;\n  }\n  .minicart__list {\n    border-bottom: 1px solid #ededed;\n    margin-bottom: 30px;\n    max-height: 250px;\n    overflow: auto;\n  }\n  .minicart__item {\n    border-bottom: 1px solid #ededed;\n    padding: 25px 0;\n  }\n  .minicart__item:last-child {\n    border-bottom: 0 none;\n  }\n  .minicart__link {\n    align-items: center;\n    display: flex;\n  }\n  .minicart__image {\n    align-items: center;\n    display: flex;\n    flex: 0 0 auto;\n    justify-content: center;\n    margin-right: 15px;\n    position: relative;\n    width: 70px;\n  }\n  .minicart__image img {\n    max-height: 60px;\n    max-width: 60px;\n  }\n  .minicart__info {\n    flex: 1 1 auto;\n  }\n  .minicart__name {\n    color: #767767;\n    display: block;\n    font-size: 13px;\n    font-weight: 600;\n    line-height: 12px;\n    margin: 0 0 5px;\n    text-transform: uppercase;\n  }\n  .minicart__details {\n    align-items: baseline;\n    display: flex;\n    justify-content: space-between;\n    margin-bottom: 10px;\n  }\n  .minicart__price,\n  .minicart__qty {\n    color: #e2b22b;\n    font-size: 13px;\n    font-weight: 600;\n    line-height: 13px;\n  }\n  .minicart__total-label {\n    color: #403232;\n    display: block;\n    font-size: 13px;\n    font-weight: 600;\n    line-height: 13px;\n    margin-bottom: 2px;\n    text-align: center;\n    text-transform: uppercase;\n  }\n  .minicart__total-price {\n    color: #e2b22b;\n    display: block;\n    font-size: 16px;\n    font-weight: 600;\n    line-height: 16px;\n    text-align: center;\n  }\n  .minicart__finish {\n    background: #75da5b;\n    border: 1px solid #75da5b;\n    border-radius: 3px;\n    box-shadow: 2px 2px 10px 0 rgba(92,19,30,0.15);\n    color: #fff;\n    display: block;\n    font-size: 16px;\n    font-weight: 600;\n    line-height: 16px;\n    margin-top: 25px;\n    padding: 16px 20px;\n    text-align: center;\n    text-transform: uppercase;\n    width: auto;\n  }\n  .minicart__finish:hover {\n    background: #fff;\n    color: #75da5b;\n  }\n}\n.minicart__header {\n  display: none;\n}\n"},"lineCount":269}},"error":null,"hash":"8d17a3f0aa4742f833ebd2439cbfc9dd","cacheData":{"env":{}}}